        <div class="col-md-4 p-3">
          <label>Nome</label>
        </div>
        <div class="col-md-4 p-3">
            <select disabled class="form-control col-md-8">
                <option value="0">No</option>
                <option value="2" selected>Sì, obbligatorio</option>
            </select>
        </div>
        <div class="col-md-4 p-3 left-border">
            <select disabled class="form-control col-md-8">
                <option value="0">No</option>
                <option value="2" selected>Sì, obbligatorio</option>
            </select>
        </div>
      </div>

